generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Adm {
  id       String @unique @default(uuid())
  name     String
  rg       Int @unique
  area     String
  email    String @unique
  password String @unique
}

model Member {
  id                    String @unique @default(uuid())
  email                 String @unique
  name                  String
  nickName              String
  cpf                   Int @unique
  rg                    Int @unique
  educationInstituition String
  ra                    Int @unique
  course                String
  yearOfGraduation      Int
  phoneNumber           Int @unique
  yearOfBirth           Int
  instagram             String @unique
  lawsLink              String
  readyLink             String
  team                  String
  shirtSize             String
  shirtNumber           Int?
}
